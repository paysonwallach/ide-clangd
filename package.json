{
  "name": "atom-ide-clangd",
  "description": "A clangd language server plugin for Atom",
  "repository": "https://github.com/jbree/ide-clangd",
  "version": "0.4.0",
  "main": "lib/main.js",
  "keywords": [
    "atom-ide",
    "ide",
    "c",
    "c++",
    "cpp",
    "clang",
    "clangd",
    "languageserver",
    "language-server-protocol",
    "language-client"
  ],
  "author": "Joshua Breeden <jbreeden@me.com>",
  "license": "GPL-3.0",
  "engines ": {
    "atom": ">=1.21.0"
  },
  "configSchema": {
    "clangExecutable": {
      "type": "string",
      "default": "/usr/local/bin/clangd",
      "description": "Absolute path to `clangd` executable."
    }
  },
  "scripts": {
    "build:meta": "scrivano",
    "format": "run-p format:*",
    "format:js": "prettier --write .",
    "format:package.json": "prettier-package-json --write",
    "release": "standard-version"
  },
  "dependencies": {
    "atom-languageclient": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.1.3",
    "scrivano": "^0.1.0",
    "standard-version": "^9.0.0"
  },
  "enhancedScopes": [
    "source.c",
    "source.cpp"
  ],
  "consumedServices": {
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "provideSignatureHelp"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "standard-version": {
    "scripts": {
      "precommit": "npm run build:meta"
    }
  }
}
